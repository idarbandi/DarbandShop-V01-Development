{
    "editor.formatOnSave": true,
    "python.pythonPath": "venv\\scripts\\python.exe",
    "python.sortImports.args": [
        "--profile=black",
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "line-length=119"
    ],
    "[python]": {
        "editor.codeActionOnSave": {
            "source.organizeImports": true,
        }
    }
}